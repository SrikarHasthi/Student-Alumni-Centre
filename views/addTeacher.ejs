<%- include("_header") -%>
<form action="" method="POST" class="student-form">
    <label for="id">ID:</label>
    <input type="number" id="id" name="id" style="margin-left: 30px;"><br><br>
    <label for="Name">Name:</label>
    <input type="text" id="name" name="name" style="margin-left: 9px;"><br><br>
    <label for="gender">Gender:</label>
    <select name="gender" id="gender">
      <option value="0">Male</option>
      <option value="1">Female</option>
      <option value="2">Other</option>
    </select><br><br>
    <label for="class">Class:</label>
    <input type="number" id="class" name="class" style="margin-left: 13px;"><br><br>
    <input type="submit" value="Submit">
  </form>
<%- include("_footer") -%>